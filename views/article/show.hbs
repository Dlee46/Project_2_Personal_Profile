<div class="row">
    <div class="col s12">
        <ul class="tabs">
            <li class="tab col s4">
                <a target="_self" href="/user/{{userId}}/organizer">Home</a>
            </li>
            <li class="tab col s4">
                <a target="_self" href="/user/{{userId}}/organizer/{{organizerId}}/article">Article</a>
            </li>
            <li class="tab col s4">
                <a target="_self" href="/">Log Out</a>
            </li>
        </ul>
    </div>
</div>
<h1>{{article.title}}</h1>

<img src="{{article.photoUrl}}" alt="{{article.title}} Image" for="photoUrl">
<p>Notes: {{article.notes}}</p>
<a href="{{article.url}}" for="url">{{article.title}} Link</a>
<form action="/user/{{userId}}/organizer/{{organizerId}}/article/{{articleId}}/edit">
    <button type="submit">Edit Information</button>
</form>