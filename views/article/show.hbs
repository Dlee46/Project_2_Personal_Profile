<div class="row">
    <div class="col s12">
        <ul class="tabs">
            <li class="tab col s4">
                <a target="_self" href="/user/{{userId}}/organizer">Home</a>
            </li>
            <li class="tab col s4">
                <a target="_self" href="/user/{{userId}}/organizer/{{organizerId}}/article">Article</a>
            </li>
            <li class="tab col s4">
                <a target="_self" href="/">Log Out</a>
            </li>
        </ul>
    </div>
</div>
<h1>{{article.title}}</h1>

<img class="materialboxed" width="650" src="{{article.photoUrl}}" alt="{{article.title}} Image" for="photoUrl">
<p>Notes: {{article.notes}}</p>
<a href="{{article.url}}" for="url">{{article.title}} Link</a>
<br>
<form action="/user/{{userId}}/organizer/{{organizerId}}/article/{{articleId}}/edit">
    <button type="submit" class="waves-effect blue accent-1 btn ">
        <i class="material-icons left ">file_upload</i>Edit Information</button>
</form>