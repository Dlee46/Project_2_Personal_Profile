<h1>Categories</h1>
<ul>

    {{#each user.organizers}}

    <input type="checkbox" class="filled-in" checked="unchecked" />
    <a href="/user/{{../userId}}/organizer/{{_id}}">{{title}}</a>
    <br>
    <form action="/user/{{../userId}}/organizer/{{_id}}/edit">
        <button type="submit" class="waves-effect blue accent-1 btn">
            <i class="material-icons left">edit</i>Edit Category</button>
    </form>
    <br>
    <form action="/user/{{../userId}}/organizer/{{_id}}?_method=DELETE" method="POST">
        <button type="submit" class="waves-effect blue accent-1 btn">
            <i class="material-icons left">delete</i>Delete</button>
    </form>
    <br> {{/each}}
</ul>
<a href="/user/{{userId}}/organizer/new" class="waves-effect blue accent-1 btn">
    <i class="material-icons left">add</i>Create a Category</a>